package net.minecraft.src;

public class CommandChickens extends CommandBase {

	public CommandChickens() {
		// TODO Auto-generated constructor stub
	}

	@Override
	public String getCommandName() {
		// TODO Auto-generated method stub
		return "chickens";
	}

	@Override
	public void processCommand(ICommandSender var1, String[] var2) {
		// TODO Auto-generated method stub
		System.out.println("Chickens!");
		EntityPlayerMP thisPlayer = this.getCommandSenderAsPlayer(var1);
		World par1World = thisPlayer.worldObj;
		for(int i = 1; i<=30; i++){
			Entity chicken = new EntityChicken(par1World);
			chicken.posX = thisPlayer.posX;
			chicken.posY = thisPlayer.posY;
			chicken.posZ = thisPlayer.posZ;
			par1World.spawnEntityInWorld(new EntityChicken(par1World));
		}
	}
	public int getRequiredPermissionLevel()
    {
        return 0;
    }

	public String getCommandUsage(ICommandSender par1ICommandSender)
    {
        return "/chickens";
    }
}
